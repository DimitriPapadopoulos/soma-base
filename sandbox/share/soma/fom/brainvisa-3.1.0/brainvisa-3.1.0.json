{
    "attributes": {
        "analysis" : {
            "descr" : "Allow to discriminate several set of parameters for the same process",
            "default_value" : "default_analysis"
        },
        "protocol" : {
            "descr" : "Name of the cuurent study", 
            "default_value" : "default_protocol"
        },
        "modality" : {
            "descr" : "Imaging modality"
        }, 
        "acquisition" : {
            "descr" : "Allows to discriminate several acquisitions of the same modality for the same subject",
            "default_value": "default_acquisition"    
        },
        "subject" : {
            "descr" : "Identifer of the subject"
        },
        "side" : {
            "type" : "Enum",
            "*args" : [ "L", "R" ],
            "descr" : "Select L for Left or R for Right"
        },
        "sulci_recognition_type" : {
            "type" : "Enum",
            "*args" : [ "auto", "manual", "best" ],
            "descr" : "Type of sulci recognition"
        },
        "graph_version" : {
            "descr" : "Version of sulci recognition graph"
        },
        "sulci_recognition_session" : {
            "descr" : "Session name for sulci recognition"
        }
    },

    "directories": {
        "dir1" : "<protocol>/<subject>/<modality>/<acquisition>"
    },

    "extensions" : {
        "image" : [ ".nii", ".ima" ],		 
        "dicom" : [ ".dcm", "" ],
        "commissures_coordinates" : [".APC"]
    },

    "processes" : {
    }

